   "source": [
    "import torch\n",
    "\n",
    "\n",
    "def coeff_of_determination(pred, target):\n",
    "    p = torch.as_tensor(pred)\n",
    "    t = torch.as_tensor(target)\n",
    "    var1 = t.var()\n",
    "    var2 = (t-p).var()\n",
    "    R2 = 1-var2/var1\n",
    "    return R2"
   ]
  }

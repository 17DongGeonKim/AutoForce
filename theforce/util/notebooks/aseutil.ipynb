   "source": [
    "from ase.io import Trajectory\n",
    "\n",
    "\n",
    "def downsize_traj(file, l, outfile):\n",
    "    t = Trajectory(file)\n",
    "    tt = Trajectory(outfile, 'w')\n",
    "    for k, atoms in enumerate(t):\n",
    "        if k == l:\n",
    "            break\n",
    "        tt.write(atoms)"
   ]
  }
